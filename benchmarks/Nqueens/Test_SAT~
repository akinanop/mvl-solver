using namespace std;

void run_solver(string file)
{ char buffer [100];
  FILE * outp;
  outp = fopen("out4_33_minisat", "a");

  string total =  "minisat "+file;
  FILE* test = popen(total.c_str(), "r");
  while ( ! feof (test) )
       {
         if ( fgets (buffer , 100 , test) == NULL ) break;
         fputs (buffer , outp);

       }
  fclose(outp);
  pclose(test);

}

